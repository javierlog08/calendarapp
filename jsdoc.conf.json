{
  "plugins": [
    "node_modules/jsdoc/plugins/markdown",
    "node_modules/angular-jsdoc/common/plugins/ngdoc"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "source": {
    "include": [ "app" ],
    "exclude": [ "app/bower_components" ],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  }
}
